<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.PostQuizController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="850.0" pickOnBounds="true">
         <image>
            <Image url="@../resources/images/background.png" />
         </image>
      </ImageView>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="850.0">
         <center>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="_nextLevelButton" mnemonicParsing="false" onAction="#nextLevelButtonPressed" prefHeight="120.0" prefWidth="150.0" style="-fx-background-radius: 50; -fx-background-color: c7f392;" text="Next Level">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="_reviewButton" mnemonicParsing="false" onAction="#reviewLevelButtonPressed" prefHeight="120.0" prefWidth="150.0" style="-fx-background-radius: 50; -fx-background-color: c7f392;" text="Review Level">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#returnToTitleButtonPressed" prefHeight="120.0" prefWidth="150.0" style="-fx-background-radius: 50; -fx-background-color: c7f392;" text="Main Menu">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="_playVideoButton" mnemonicParsing="false" onAction="#playVideoButtonPressed" prefHeight="120.0" prefWidth="150.0" style="-fx-background-radius: 50; -fx-background-color: c7f392;" text="Play Video">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
               <BorderPane.margin>
                  <Insets bottom="50.0" />
               </BorderPane.margin>
            </HBox>
         </center>
         <top>
            <VBox alignment="CENTER" prefWidth="100.0" spacing="30.0" BorderPane.alignment="CENTER">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="VOXSPELL Level Complete">
                     <font>
                        <Font name="System Bold" size="36.0" />
                     </font>
                  </Text>
                  <VBox alignment="CENTER" spacing="30.0">
                     <children>
                        <Label fx:id="_userResultsOne" text="Display some stats here or something" textFill="WHITE">
                           <font>
                              <Font name="System Bold" size="18.0" />
                           </font>
                        </Label>
                        <Label fx:id="_userResultsTwo" layoutY="57.0" text="Display some stats here or something" textFill="WHITE">
                           <font>
                              <Font name="System Bold" size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <VBox.margin>
                        <Insets top="120.0" />
                     </VBox.margin>
                  </VBox>
               </children>
               <padding>
                  <Insets top="50.0" />
               </padding>
            </VBox>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
